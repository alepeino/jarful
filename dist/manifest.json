{
  "name": "Jarful",
  "description": "Copy the dependency vector from a Clojars badge to the clipboard.",
  "manifest_version": 2,
  "version": "0.1",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://github.com/*"],
      "js": ["main.js"]
    }
  ],
  "background": {
    "scripts": ["bg.js"],
    "persistent": false
  },
  "permissions": ["notifications"]
}
